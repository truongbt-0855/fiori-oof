{
  "name": "com.internal.oof",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ui5 serve -o index.html --config ui5-local.yaml",
    "start-mock": "ui5 serve -o test/mockServer.html",
    "build": "ui5 build --clean-dest --dest dist --include-task=generateManifestBundle generateCachebusterInfo",
    "build:opt": "ui5 build self-contained --clean-dest --dest dist",
    "serve-dist": "ws --compress -d dist --open",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
    "build:mta": "rimraf resources mta_archives && mbt build",
    "deploy1": "fiori cfDeploy --version-rule ALL",
    "deploy": "cf deploy mta_archives/fiori-oof_1.0.0.mtar"
  },
  "description": "",
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1.19.1",
    "@sapui5/ts-types": "^1.134.0",
    "@ui5/cli": "^4.0.14",
    "rimraf": "^5.0.0"
  },
  "dependencies": {
    "@sap/ui5-builder-webide-extension": "^1.1.9",
    "ui5-task-zipper": "^3.4.1"
  }
}
